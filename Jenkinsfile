                echo 'Hello World'
            }
        }
        stage('TEST') {
            steps {
                sh 'sleep 5'
                echo 'Hello World'
            }
        }
        stage('DEPLOY') {
            steps {
                sh 'sleep 5'
                echo 'Hello World'
            }
        }
    }
}
